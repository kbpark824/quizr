{
  "expo": {
    "name": "Quizr",
    "slug": "quizr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quizr",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "projectId": "${EXPO_PUBLIC_EAS_PROJECT_ID}",
    "ios": {
      "bundleIdentifier": "app.quizr",
      "supportsTablet": true
    },
    "android": {
      "package": "app.quizr",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "navigationBarColor": "#000000",
      "navigationBarStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "${EXPO_PUBLIC_EAS_PROJECT_ID}"
      }
    },
    "owner": "kbpark824"
  }
}
